{
  "testEnvironment": "jest-environment-node",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(mjs?|jsx?|js?|tsx?|ts?)$",
  "testPathIgnorePatterns": [
    "<rootDir>/build/",
    "<rootDir>/node_modules/"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx",
    "mjs"
  ]
}